<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<!-- Alias(객체명) 설정 -->
	<typeAliases>
		<typeAlias type="com.ggul.zip.lesson.LessonVO" alias="lesson" />
		<typeAlias type="com.ggul.zip.user.UserVO" alias="users" />
		<typeAlias type="com.ggul.zip.notice.NoticeVO" alias="notice"/>
		<typeAlias type="com.ggul.zip.faq.FaqVO" alias="faq"/>
		<typeAlias type="com.ggul.zip.queen.QueenVO" alias="queen"/>
		<typeAlias type="com.ggul.zip.message.MessageTO" alias="message" />
		<typeAlias type="com.ggul.zip.lesson.LessonDetailVO" alias="lessonDetail" />
		<typeAlias type="com.ggul.zip.lesson.ReviewVO" alias="review" />
		<typeAlias type="com.ggul.zip.tiper.TiperVO" alias="tiper" />
	</typeAliases>

<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/search-mapper.xml" />
		<mapper resource="mappings/main-mapper.xml" />
		<mapper resource="mappings/user-mapper.xml" />
		<mapper resource="mappings/queen-mapper.xml" />
		<mapper resource="mappings/notice-mapper.xml"/>
		<mapper resource="mappings/faq-mapper.xml"/>
		<mapper resource="mappings/message-mapping.xml" />
		<mapper resource="mappings/lessonDetail-mapper.xml" />
		<mapper resource="mappings/lesson-mapper.xml" />
		<mapper resource="mappings/tiper-mapper.xml" />
	</mappers>

</configuration>